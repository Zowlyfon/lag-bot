{
  "name": "lag-bot",
  "version": "1.0.0",
  "description": "Discord bot for gnulag",
  "main": "build/index.js",
  "scripts": {
    "build:typescript": "npx tsc",
    "run:node": "node build/src/index.js",
    "dev": "nodemon --watch src/ -e ts --exec ts-node src/index.ts",
    "run": "npm run build:typescript && npm run run:node",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "deploy": "npm run build:typescript && node build/scripts/deploy-commands.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "dexie": "^4.0.0-alpha.4",
    "discord.js": "^14.2.0",
    "dotenv": "^16.0.1",
    "leveldown": "^6.1.1",
    "pouchdb-adapter-leveldb": "^7.3.0",
    "reflect-metadata": "^0.1.13",
    "rxdb": "^12.7.16",
    "rxjs": "^7.5.6",
    "typedi": "^0.10.0"
  }
}
